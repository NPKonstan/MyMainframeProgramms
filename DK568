 DCL DATEX CHAR(6); DATEX = DATE();
     /*’–œÀœ√…”Ãœ” «Ã≈—ŸÕ –—œ«√œ’Ã≈Õœ’ Ã«Õ¡      */
     TODAYX            = '20'!!
                          SUBSTR(DATEX,1,2) !! /*ETOS */
