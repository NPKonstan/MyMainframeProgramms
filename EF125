 DCL PROM_ORGATM(99999)   PIC'999999'    INIT((99999)0);
 DCL II              PIC'99999'    INIT(0);
  /*************  cPAMMH  EKTYpxrHr   ********************************/
  DCL 1 LIN,
        2 CTR   CHAR(1)   INIT('1')     ,
        2 GRAMM CHAR(132) INIT((132)' ');
  /*************  cPAMMH  EKTYpxrHr   ********************************/
    DCL  ENA  CHAR(24)
        VALUE('ABcdEZHhIKkMNnOpPrTYuXwx');
    DCL  DYO  CHAR(24)
        VALUE('абцдефгхийклмнопястужвьы');


   DCL   1   RECREAD,
             2 C1        CHAR(1)         INIT(' '),
             2 CORG      CHAR(5)         INIT(' '),
             2 C2        CHAR(1)         INIT(' '),
             2 CPROM     CHAR(4)         INIT(' '),
             2 C3        CHAR(69)        INIT(' ');

 %PAGE;
   DCL   1   BATHEAD,
             2 ITRXCOD   CHAR(2)         INIT('BH'),
             2 ICOMP     CHAR(4)         INIT('CEUR'),
             2 ISYSNO    CHAR(4)         INIT('  01'), /*DES MESA */
