          PINAX(I).PROD_TYPE(13).PP(1).CHR_PROD ='–—œÕ.–¡ ≈‘œ’ ≈’–œ—«”';
          PINAX(I).PROD_TYPE(13).PP(1).COD_PROD_FAMILY = '1000';
          PINAX(I).PROD_TYPE(13).PP(1).CHR_PROD_FAMILY =
