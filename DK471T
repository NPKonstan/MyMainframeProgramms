       2  WS_PIC9_KATHOL PIC '(4)9'  ,
       2  WS_99          PIC '99'    ;

 DCL  WS_KATHOL     PIC '(4)9' INIT (0);

 DCL  WS_KATH_OLD   PIC'(6)9'  INIT (931900);
