                  PINAX(I).PROD_TYPE(1).PP(1).NUM_SALES_CNT =
                  PINAX(I).PROD_TYPE(1).PP(1).NUM_SALES_CNT + 1;
                  PINAX(I).PROD_TYPE(1).PP(1).NUM_SALES_AMT =
                  PINAX(I).PROD_TYPE(1).PP(1).NUM_SALES_AMT +
                                          KSRDYPOL + KSREKKRE ;
