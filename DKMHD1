         2   HL_1        CHAR ( 08 ) INIT ( ' SEGMENT' ) ,              04100011
         2   HL_3        CHAR ( 19 ) INIT ( '        ƒ…¡¬¡”‘« ¡Õ' )  ,  04110011
         2   HL_5        CHAR ( 19 ) INIT ( ' √—¡÷‘« ¡Õ ( À≈…”.)' )  ,  04120011
