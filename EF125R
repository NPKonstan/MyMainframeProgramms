   DCL 1 LINE3,
         2 L3SKIP  CHAR(01)   INIT(' '),
         2 L3FIL1  CHAR(10)   INIT('E÷¡—Ãœ√H :'),
         2 L3FIL2  CHAR(122)   INIT((122)' ');
   DCL 1 LINE4,
         2 L4SKIP  CHAR(01)   INIT(' '),
         2 L4FIL1  CHAR(132)
           INIT('Àœ√…”‘… ≈” E√√—¡÷≈” √IA TO MSA (KATMA,INT,ATM,APS)');
   DCL 1 LINE6,
         2 L6FIL1 CHAR(01)    INIT(' '),
