   DCL 1 LINE8,
         2 L8FIL1  CHAR(01)   INIT(' '),
         2 L8FIL3  CHAR(132)   INIT((132)'-');
   DCL 1 LINE17,
         2 L17FIL1  CHAR(01)             INIT('0'),
         2 L17FIL2  CHAR(12)             INIT('”’ÕœÀO RECOR'),
         2 L17FIL3  CHAR(18)             INIT('DS –œY √—¡÷THKAN  '),
