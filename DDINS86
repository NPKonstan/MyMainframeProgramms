         RC = 1;                                                        04260010
         MSG='TO педио CTL_TT тавудяолийос йыдийас '!!                  04270010
             'EINAI кахос,пеяиевеи лг аяихлгтийа ьгжиа '!!              04280010
              CTL_TT  ;                                                 04290010
       ERLIN2=MSG;                                                      04291024
         RETURN (RC) ;                                                  04300010
  END;                                                                  04310010
  IF (CTL_COUNTRY         ^= 'GR' ) THEN DO;                            04320010
         RC = 1;                                                        04330010
         MSG='TO педио CTL_COUNTRY йыдийос выяас '!!                    04340010
             'EINAI кахос,пяепеи ма еимаи GR Aкка еимаи '!!             04350010
              CTL_COUNTRY;                                              04360010
       ERLIN2=MSG;                                                      04361024
         RETURN (RC) ;                                                  04370010
  END;                                                                  04380010
                                                                        04410010
  IF VALID(CTL_COMM_CUST) = '0'B    THEN DO;                            04420029
         RC = 1;                                                        04421029
         MSG='TO педио CTL_COMM_CUST пяолгхеиа пекатг '!!               04422029
             'EINAI кахос,евеи кахос FORMAT '!!                         04423029
              CTL_COMM_CUST;                                            04424029
       ERLIN2=MSG;                                                      04425029
       RETURN (RC) ;                                                    04426029
  END;                                                                  04427029
  IF VALID(CTL_COMM_COMP) = '0'B    THEN DO;                            04430029
         RC = 1;                                                        04440029
         MSG='TO педио CTL_COMM_COMP пяолгхеиа ояцамислоу '!!           04450029
             'EINAI кахос,евеи кахос FORMAT '!!                         04460029
              CTL_COMM_COMP;                                            04470029
       ERLIN2=MSG;                                                      04480029
       RETURN (RC) ;                                                    04490029
  END;                                                                  04500029
      RETURN (RC) ;                                                     04540010
    END VALIDATE_READER;                                                04550010
   /**************************************************************/     04560010
