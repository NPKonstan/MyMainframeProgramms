    /*----------------------------------------------------------*/      02010000
    /*                    MAIN  PROGRAM                         */      02020000
    /*----------------------------------------------------------*/      02030000
    OPEN FILE(PRT) ;                                                    02040000
    OPEN FILE ( READER  );                                              02050000
                                                                        02060000
   DO WHILE (EOF_READER = 0);                                           02070003
      CALL READ_READER;                                                 02080013
   END;                                                                 02090000
