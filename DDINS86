   /**************************************************************/     02110008
   /*READ_READER START                                           */     02120008
   /**************************************************************/     02130008
    READ_READER: PROC;                                                  02140003
      DCL ERR_MSG CHAR(132);                                            02150008
                                                                        02160008
      READ  FILE ( READER  ) INTO ( CTL_CARD1 );                        02170003
      IF EOF_READER = 1 THEN DO;                                        02171011
         GOTO READ_TELOS;                                               02171111
      END;                                                              02172011
      CNT_Records_READ = CNT_Records_READ + 1;                          02180008
      READ  FILE ( READER  ) INTO ( CTL_CARD2 );                        02190003
      CNT_Records_READ = CNT_Records_READ + 1;                          02200008
      READ  FILE ( READER  ) INTO ( CTL_CARD3 );                        02210003
      CNT_Records_READ = CNT_Records_READ + 1;                          02220008
      READ  FILE ( READER  ) INTO ( CTL_CARD4 );                        02230008
      CNT_Records_READ = CNT_Records_READ + 1;                          02240008
      READ  FILE ( READER  ) INTO ( CTL_CARD5 );                        02250008
      CNT_Records_READ = CNT_Records_READ + 1;                          02260008
      READ  FILE ( READER  ) INTO ( CTL_CARD6 );                        02270008
      CNT_Records_READ = CNT_Records_READ + 1;                          02280008
      READ  FILE ( READER  ) INTO ( CTL_CARD7 );                        02290008
      CNT_Records_READ = CNT_Records_READ + 1;                          02300008
      READ  FILE ( READER  ) INTO ( CTL_CARD8 );                        02301020
      CNT_Records_READ = CNT_Records_READ + 1;                          02302020
      READ  FILE ( READER  ) INTO ( CTL_CARD9 );                        02303022
      CNT_Records_READ = CNT_Records_READ + 1;                          02304022
      READ  FILE ( READER  ) INTO ( CTL_CARD10);                        02305022
      CNT_Records_READ = CNT_Records_READ + 1;                          02306022
      READ  FILE ( READER  ) INTO ( CTL_CARD11);                        02307022
      CNT_Records_READ = CNT_Records_READ + 1;                          02308022
      READ  FILE ( READER  ) INTO ( CTL_CARD12);                        02309022
      CNT_Records_READ = CNT_Records_READ + 1;                          02309122
      READ  FILE ( READER  ) INTO ( CTL_CARD13);                        02309222
      CNT_Records_READ = CNT_Records_READ + 1;                          02309322
      READ  FILE ( READER  ) INTO ( CTL_CARD14);                        02309422
      CNT_Records_READ = CNT_Records_READ + 1;                          02309522
      READ  FILE ( READER  ) INTO ( CTL_CARD15);                        02309622
      CNT_Records_READ = CNT_Records_READ + 1;                          02309722
      READ  FILE ( READER  ) INTO ( CTL_CARD16);                        02309822
      CNT_Records_READ = CNT_Records_READ + 1;                          02309922
      READ  FILE ( READER  ) INTO ( CTL_CARD17);                        02310022
      CNT_Records_READ = CNT_Records_READ + 1;                          02310122
      READ  FILE ( READER  ) INTO ( CTL_CARD18);                        02310229
      CNT_Records_READ = CNT_Records_READ + 1;                          02310329
      READ  FILE ( READER  ) INTO ( CTL_CARD19);                        02310429
      CNT_Records_READ = CNT_Records_READ + 1;                          02310529
                                                                        02311022
      IF VALIDATE_READER(ERR_MSG) = 1 THEN                              02341024
            CALL ERROR1(ERR_MSG);                                       02344024
      ELSE                                                              02350024
         IF CTL_CHAR1 = 'I' THEN                                        02351012
            CALL INSERT_CTDDOR86 ;                                      02360012
         ELSE                                                           02360112
         IF CTL_CHAR1 = 'U' THEN                                        02360215
            CALL UPDATE_CTDDOR86 ;                                      02360315
   /*    ELSE                                                           02360420
         IF CTL_CHAR1 = 'D' THEN                                        02360515
            CALL DELETE_CTDDOR86 ; NPK 21-02-2010 DONT DELETE*/         02361020
    READ_TELOS:                                                         02380011
    END READ_READER;                                                    02381011
   /**************************************************************/     02390008
   /*READ_READER START                                           */     02400008
   /**************************************************************/     02410008
                                                                        02420008
   /**************************************************************/     02430008
   /*VALIDATE_READER START                                       */     02440008
   /**************************************************************/     02450008
                                                                        02460008
    VALIDATE_READER: PROC (MSG) RETURNS(FIXED(1));                      02470008
      DCL RC FIXED(1);                                                  02480008
      DCL MSG CHAR(132);                                                02490008
                                                                        02500008
      MSG='';                                                           02510008
      RC=0;                                                             02520008
                                                                        02530008
      IF (MOD(CNT_Records_READ,19 ) ^= 0) THEN DO;                      02540029
         RC = 1;                                                        02550008
         MSG='о аяихлос тым еццяажым поу диабасес дем еимаи 19'         02560029
         !! ' акка : '!!MOD(CNT_Records_READ,19);                       02570029
       ERLIN2=MSG;                                                      02571024
         RETURN (RC) ;                                                  02580008
      END;                                                              02590008
                                                                        02600008
      IF ( CTL_CHAR1 ^= 'I' & CTL_CHAR1 ^= 'U'                          02610020
         )                   THEN DO;                                   02611020
         RC = 1;                                                        02620008
