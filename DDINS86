 ERROR1:PROC (MSG  ) ;                                                  05610009
    DCL MSG      CHAR  (132)  ;                                         05620000
    DCL WS_BOOM   FIXED(1) INIT(1);                                     05630000
    DCL WS_ZERO   FIXED(1) INIT(0);                                     05640000
                                                                        05650000
       ERLIN1='1';                                                      05660000
       ERLIN2='***************************************************';    05670000
       WRITE FILE(PRT) FROM(ERLINE);                                    05680000
       ERLIN1='0';                                                      05690000
                                                                        05700000
       ERLIN2=' * DDINS86  глеяа яогс         = '!!DATE ();             05710001
       WRITE FILE(PRT) FROM(ERLINE);                                    05720000
       ERLIN1=' ';                                                      05730000
                                                                        05740000
       ERLIN2='пяобкгла сто пяоцяалла DDINS86 Tо пяоцяалла ха сйасеи';  05750008
       WRITE FILE(PRT) FROM(ERLINE);                                    05760000
       ERLIN2=MSG;                                                      05770000
       WRITE FILE(PRT) FROM(ERLINE);                                    05780000
       WS_BOOM  = WS_BOOM/WS_ZERO ;                                     05790008
   END ERROR1       ;                                                   05800000
