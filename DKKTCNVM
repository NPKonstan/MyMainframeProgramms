      CLOSE CNV97_CURS;

   IF SQLCODE ^= 0  THEN BEGIN;
            CTLCHR1 = ' ';
            GRAMMH='ERROR IN CLOSING CNV97_CURS = '!!SQLCODE;
            WRITE FILE(PRINT) FROM(LINE);
   END;

