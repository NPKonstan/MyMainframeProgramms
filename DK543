   WRITE FILE (PRINTF) FROM (LINE);

   CTLCHR = ' ';
   WREC = MEG_EPIXEIR ;
   GRAMH = '**rYNOkIKA dIABArTHKAN  '!! WREC !! ' ME√¡À≈” ≈–…◊'!!
           '  STON  ‘04FUL50 ' ;
   WRITE FILE (PRINTF) FROM (LINE);

   CTLCHR = ' ';
   WREC = COUNT_MATCH ;
   GRAMH = '**  –À«»œ” Ã≈√¡À.  ≈–…◊ ”‘œÕ ‘04FUL50 –œ’ ≈◊œ’Õ  '!!
            ' ‘œ’À¡◊…”‘œÕ ≈Õ¡ Àœ√/Ãœ  ¡‘¡»≈”≈ŸÕ  '!! WREC;
   WRITE FILE (PRINTF) FROM (LINE);
   CTLCHR = ' ';
   WREC = KATATH ;
   GRAMH = '**  AYTES ≈◊œ’Õ ”’ÕœÀ… ¡  '!! WREC !! 'Àœ√¡—.  ¡‘¡»≈”≈ŸÕ';
   WRITE FILE (PRINTF) FROM (LINE);

   CTLCHR = ' ';
   WREC = COUNT_NO_MATCH ;
