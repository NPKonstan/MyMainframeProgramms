 NXTKAT:                                /* ƒIABA”MA E–OMENOY SEGMENT */
         EXEC DLI
              GN      USING PCB(3)
              INTO    (WRKAREA);

         IF DIBSTAT='GB'    THEN GOTO EXIT1;
         IF DIBSTAT^=' ' & DIBSTAT^='GA' & DIBSTAT^='GK'
         THEN DO;
                 GRAMH = ' ERROR READ D/B KAT ' !! KSRARLOG !! ' '
                                                !! DIBSTAT ;
                 WRITE FILE (ERRMSG) FROM (LINE);
                 REC_ERROR = REC_ERROR + 1;
                 SWERROR = 1;
                 GOTO NXTKAT;
         END;

         REC_TOT=REC_TOT+1;
         IF  DIBSEGM='KROOTSEG'
         THEN DO;
                  REC_ROOT=REC_ROOT+1;
                  IF  KSRARLOG=999999999         /* CONTROL RECORD  */
                  THEN DO;
                           REC_CONTROL=REC_CONTROL+1;
                           GOTO  NXTKAT;
                  END;
                  IF  BIT_LOG ='00000000'B            /* SAFETIES  */
                  THEN DO;
                           REC_SAFETY=REC_SAFETY+1;
                           GOTO  NXTKAT;
                  END;
                  IF KSRFN='u' & KSRAKYR = 0
                  THEN DO;
                         MAIN_BENEF = 'Y' ;
                         CALL ROUT_SELECT_CTBKACCN ;
                         IF BKACCN.IND_COMMON = 1 !
                            BKACCN.IND_COMMON = 2
                         /* ÍÔÈÌÔÈ Á  ÏÁ ÍÔÈÌÔÈ ÎÔ„/ÏÔÈ */
                         THEN DO;
                              PLITHOS =  0 ;
                              CALL  COUNT_DIKAIOYX;
                              IF PLITHOS  =    1  &
                                 MAIN_BENEF = 'Y'    THEN DO;
                                 CTLCHR = ' ';
                                 PR_LOGAR = WS_KEYARLOG   ;
                                 GRAMH =  (05)'*'!! PR_LOGAR   !!
                                        '  '!!KSRKATHO ;
                                 WRITE FILE (PRINT1) FROM (LINE);
                                 KOINOI_ME_ENA_DIKAIOYX =
                                 KOINOI_ME_ENA_DIKAIOYX + 1;
                                 IF EPEXERG = 'UP' THEN DO ;
                                    BKACCN.IND_COMMON   = 0;
                                    CALL UPD_BKACCN  ;
                                 END;
                              END ;
                              IF PLITHOS  =    0     THEN DO;
                                 CTLCHR = ' ';
                                 PR_LOGAR = WS_KEYARLOG   ;
                                 GRAMH =  (05)'*'!! PR_LOGAR   !!
                                        '  '!!KSRKATHO ;
                                 WRITE FILE (PRINT3) FROM (LINE);
                                 KOINOI_ME_0_DIKAIOYX =
                                 KOINOI_ME_0_DIKAIOYX  +  1  ;
                              END ;
                         END ;

                         IF BKACCN.IND_COMMON = 0
                         /* ·ÙÔÏÈÍÔÈ  ÎÔ„/ÏÔÈ */
                         THEN DO;
                              PLITHOS =  0 ;
                              CALL  COUNT_DIKAIOYX;
                              IF PLITHOS  >  1  THEN DO;
                                 CTLCHR = ' ';
                                 PR_LOGAR = WS_KEYARLOG   ;
                                 GRAMH =  (05)'*'!! PR_LOGAR         !!
                                        '  '!!KSRKATHO ;
                                 WRITE FILE (PRINT2) FROM (LINE);
                                 ATOMIKOI_AT_LEAST_2 =
                                 ATOMIKOI_AT_LEAST_2  +  1  ;
                                 IF EPEXERG = 'UP' THEN DO ;
                                      BKACCN.IND_COMMON   = 1;
                                      CALL UPD_BKACCN  ;
                                 END ;
                              END ;
                              IF PLITHOS  =  0  THEN DO;
                                 CTLCHR = ' ';
                                 PR_LOGAR = WS_KEYARLOG   ;
                                 GRAMH =  (05)'*'!! PR_LOGAR         !!
                                        '  '!!KSRKATHO ;
                                 WRITE FILE (PRINT4) FROM (LINE);
                                 ATOMIKOI_ME_0   = ATOMIKOI_ME_0
                                 +  1  ;
                              END ;
                         END;
                  END; /* IF KSRFN='U' & KSRAKYR = 0 */
 %SKIP(2);
        END ;  /*  DIBSEGM='KROOTSEG' */
      GOTO NXTKAT;
 %PAGE;
 /*-------------------------------------------------*/
 UPD_BKACCN: PROC;
                                                                        01020000
   EXEC SQL
       UPDATE CTBKACCN
       SET
          IND_COMMON       = :BKACCN.IND_COMMON
       WHERE
          CTBKACCN.COD_EFARM =  '70'               AND
          CTBKACCN.KEY_ARLOG = :WS_KEYARLOG  ;

   IF SQLCODE = 100 THEN DO;
      ACTION = 'ƒ≈Õ ’–¡—◊≈… Àœ√/Ãœ”' ;
      TABLE_ACCESS = 'CTBKACCN';
      CALL ROUT_DB2_ERROR;

   END;

   IF SQLCODE ^= 0 THEN DO;         /* –—œ¬À«Ã¡ ”‘O CTBKACCN */
      TABLE_ACCESS = 'CTBKACCN';
      CALL ERRHANDLING  ;
   END;

 END UPD_BKACCN;
 /*-------------------------------------------------*/
 COUNT_DIKAIOYX : PROC ;

   TEMP_ARLOG = KSRARLOG;
   W_ARLOG1 = TEMP_ARLOG;
   W_ARLOG2 = '';
   W_ARLOG3 = '%%';

   EXEC SQL  OPEN CURS_CNV;
   /*
    trace_mes =  ' AFTER  OPEN  CURSOR  '!! SQLCODE ;
    EXEC  SQL WRITEQ TS QUEUE
    ('DIMI') FROM (trace_mes)  LENGTH(50); */

   IF SQLCODE ^= 0 THEN DO;
      TABLE_ACCESS = '‘04CNV97';
      IF SQLCODE = 100 THEN DO;
         ACTION = 'ƒ≈Õ ¡Õœ…√≈… CURSOR ';
         CALL ROUT_DB2_ERROR;
      END;
      ELSE  DO;
           ACTION = 'PROBLEM IN TABLE ';
           CALL ROUT_DB2_ERROR;
      END ;
   END;


   EOCURS = '0'B; CursRows       = 0;
   DO WHILE (   ^ EOCURS             ) ;
      CALL ROUT_FETCH;
   END;

   EXEC SQL
      CLOSE CURS_CNV;

   IF SQLCODE ^= 0 THEN DO;
      TABLE_ACCESS = '‘04CNV97';
      IF SQLCODE = 100 THEN DO;
           ACTION = 'ƒ≈Õ  À≈…Õ≈… CURSO';
           CALL ROUT_DB2_ERROR;
      END;
      ELSE  DO;
           ACTION = 'PROBLEM IN TABLE ';
           CALL ROUT_DB2_ERROR;
      END ;
   END;

   PLITHOS = CURSROWS ;


 END  COUNT_DIKAIOYX ;
  /* ---------------------------------------------- */
 /*  +++++++++++++++++++++++++++++++++++++++++++++++ */
 ROUT_FETCH : PROC;
   EXEC SQL
        FETCH CURS_CNV
        INTO
           :T04CNV97.COD_PELATH,
           :T04CNV97.COD_EFARM,
           :T04CNV97.NUM_AR_LOGAR,
           :T04CNV97.COD_KATMA,
           :T04CNV97.IND_OPEN_CLOSE;

   IF SQLCODE = 100 THEN DO;
      EOCURS = '1'B;
      GO TO END_FETCH;
   END;

   IF SQLCODE ^= 0 THEN DO;
      TABLE_ACCESS = 'T04CNV97';
      CALL  ERRHANDLING ;
   END;

   CursRows     =  CursRows + 1 ;
   IF   SYN02  >  01  & CursRows = 1    THEN DO;
      MAIN_BENEF = 'N' ;
      PR_LOGAR = WS_KEYARLOG  ;
      GRAMH =  (05)'*'!! PR_LOGAR !!'  ƒ≈Õ ≈◊≈…  ’—…œ ƒ… ¡…œ’◊œ ”‘œ '!!
                ' CIDB ' ;

      WRITE FILE (ERRMSG) FROM (LINE);

   end ;



 END_FETCH:
 END ROUT_FETCH;
       /*------------------------------------------------*/
 /*-------------------------------------------------*/
 /*-------------------------------------------------*/
 ROUT_SELECT_CTBKACCN : PROC;

   WS_KEYARLOG = KSRARLOG;

   EXEC SQL
       SELECT       IND_COMMON
       INTO
          :BKACCN.IND_COMMON
       FROM    CTBKACCN
       WHERE   COD_EFARM = '70'        AND
               KEY_ARLOG = :WS_KEYARLOG
       WITH UR;


   IF SQLCODE = 100 THEN
   DO;
         BKACCN.IND_COMMON = 9 ; /* DUMMY √…¡ Õ¡ Ã«Õ ‘’–ŸÕ≈… œ‘…
                                   ƒ«»≈Õ ƒ…œ—»Ÿ»« ≈ */
         ACTION =  WS_KEYARLOG !!  'NOT FOUND IN ';
         TABLE_ACCESS = 'CTBKACCN';
         CALL ROUT_DB2_ERROR;
   END;
   IF SQLCODE ^= 0 THEN DO;
      BKACCN.IND_COMMON = 9 ; /* DUMMY √…¡ Õ¡ Ã«Õ ‘’–ŸÕ≈… œ‘…
                                   ƒ«»≈Õ ƒ…œ—»Ÿ»« ≈ */
      TABLE_ACCESS = 'CTBKACCN';
      CALL  ERRHANDLING  ;                /* –—œ¬À«Ã¡ ”‘O CTBKACCN */
   END;

 END ROUT_SELECT_CTBKACCN;

 /*******************************************************************/
 ERRHANDLING : PROC ;
         ACTION = 'PROBLEM IN TABLE ';
         CALL ROUT_DB2_ERROR;
 END ERRHANDLING ;
 /* -------------------------------------------------------------- */   03700100
 ROUT_DB2_ERROR: PROC;                                                  03702000
                                                                        03706000
           RET_COD = SQLCODE;                                           03707000
           GRAMH     = '  '!!LT_ASTER !!                                03708000
                       LT_SQLSTATE_ERR !! SQLSTATE !!' '!!              03709000
                       LT_SQLCODE_ERR !! RET_COD !!' '!!                03709100
                       LT_TABLE_ACCESS !! TABLE_ACCESS !!               03709200
                       LT_ACTION !! ACTION ;                            03709300
          CALL PUTLAUH;                                                 03709600
  END ROUT_DB2_ERROR;                                                   03709800

 /* -------------------------------------------------------------- */

 PUTLAUH: PROC ;
         WRITE FILE(ERRMSG) FROM(LINE) ;
         GRAMH   = (132)' ' ;
         END PUTLAUH ;



 /*  +++++++++++++++++++++++++++++++++++++++++++++++ */
  /* ----------------------------------------------------------- */
 EXIT1:
   CTLCHR = ' ';
   GRAMH =  (100)'-' ;
   WRITE FILE (PRINT1) FROM (LINE);

   CTLCHR = ' ';
   GRAMH =  (100)'-' ;
   WRITE FILE (PRINT2) FROM (LINE);

   CTLCHR = ' ';
   WREC = REC_TOT;
   GRAMH = '**rYNOkIKA dIABArTHKAN     '!! WREC;
   WRITE FILE (PRINT1) FROM (LINE);

   CTLCHR = ' ';
   WREC = REC_ROOT;
   GRAMH = '**ROOT SEGMENTS            '!! WREC;
   WRITE FILE (PRINT1) FROM (LINE);

   WREC = REC_CONTROL;
   GRAMH = '**ROOT CONTROL RECORD      '!! WREC;
   WRITE FILE (PRINT1) FROM (LINE);

   WREC = REC_SAFETY;
   GRAMH = '**ROOT SEGMENTS - SAFETIES '!! WREC;
   WRITE FILE (PRINT1) FROM (LINE);


   WREC = KOINOI_ME_ENA_DIKAIOYX ;
   GRAMH = '**KOINOI Àœ√/Ãœ… Ã≈  1       ƒ… ¡…œ’◊œ '!!
           ' –œ’ ƒ…œ—»Ÿ»« ≈ « ≈Õƒ≈…Œ«  '!!WREC;
   WRITE FILE (PRINT1) FROM (LINE);


   WREC = KOINOI_ME_0_DIKAIOYX ;
   GRAMH = '**KOINOI Àœ√/Ãœ… Ã≈  0    ƒ… ¡…œ’◊œ’” ' !!WREC;
   WRITE FILE (PRINT3) FROM (LINE);


   WREC = ATOMIKOI_AT_LEAST_2 ;
   GRAMH = '**¡‘œÃ… œ… Àœ√/Ãœ… Ã≈  >1       ƒ… ¡…œ’◊œ’” '!!
           ' –œ’ ƒ…œ—»Ÿ»« ≈ « ≈Õƒ≈…Œ«  '!!WREC;
   WRITE FILE (PRINT2) FROM (LINE);


   WREC = ATOMIKOI_ME_0    ;
   GRAMH = '**¡‘œÃ… œ… Àœ√/Ãœ… Ã≈  0   ƒ… ¡…œ’◊œ’” '!!WREC;

   WRITE FILE (PRINT4) FROM (LINE);




   CLOSE FILE(PRINT1),  FILE(PRINT2) ; CLOSE FILE(CARD  ) ;
   CLOSE FILE(PRINT3),  FILE(PRINT4),  FILE(ERRMSG)  ;
  END  DK537;
