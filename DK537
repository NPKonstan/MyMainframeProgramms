                 '”≈ ¡‘œÃ… œ’”' ;
         WRITE FILE (PRINT1)    FROM(LINE);
         GRAMH=' '!!(100)'-';
         WRITE FILE (PRINT1)    FROM(LINE);


         GRAMH = '   œ…Õœ… H MH KOINOI Àœ√¡—…¡”Ãœ…  '!!
                 'Ã≈ 0 ƒ… ¡…œ’◊œY”   ';
         WRITE FILE (PRINT3)    FROM(LINE);
         GRAMH=' '!!(100)'-';
         WRITE FILE (PRINT3)    FROM(LINE);


         GRAMH = '  ATOMIKOI  Àœ√¡—…¡”Ãœ… ME ‘œ’À¡◊…”‘œÕ  '!!
                 'ƒ’œ  ƒ… ¡…œ’◊œ’” –œ’ ƒ…œ—»Ÿ»« ≈ « ≈Õƒ≈…Œ«  '!!
                 '”≈  œ…Õœ’”' ;
         WRITE FILE (PRINT2)    FROM(LINE);
